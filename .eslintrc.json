{
	"extends": [
		"next",
		"next/core-web-vitals",
		"prettier",
		"plugin:prettier/recommended",
		"eslint:recommended"
	],
	"rules": {
		"no-console": ["error", { "allow": ["warn", "error", "info"] }],
		"prefer-const": "error",
		"no-unused-vars": "warn",
		"no-irregular-whitespace": "off"
	}
}
